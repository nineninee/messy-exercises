  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/src/design-pattern/test.ts",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch ts with pre",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/design-pattern/index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
  ]
}
